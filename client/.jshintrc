        "_"          : false,
        "describe"   : false,
        "it"         : false,
        "before"     : false,
        "beforeEach" : false,
        "after"      : false,
        "afterEach"  : false,
        "expect"     : false,
        "ngMidwayTester" : false,
        "$$" : false,
        "browser" : false,
        "pending" : false,
        "inject": false,
        "spyOn": false
